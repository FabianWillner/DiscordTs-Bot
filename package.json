{
    "name": "discordts-bot",
    "version": "1.0.0",
    "description": "A simple Javascript Discord Bot",
    "main": "tsc & node main.js",
    "type": "module",
    "dependencies": {
        "@discordjs/builders": "^0.9.0",
        "@discordjs/rest": "0.1.0-canary.0",
        "@discordjs/voice": "^0.7.5",
        "discord-api-types": "^0.25.2",
        "discord.js": "^13.3.1",
        "eslint": "^8.4.1",
        "ffmpeg-static": "^4.4.0",
        "libsodium-wrappers": "^0.7.9",
        "node-fetch": "^3.1.1",
        "opusscript": "0.0.8",
        "winston": "^3.3.3",
        "youtube-search": "^1.1.6",
        "ytdl-core": "^4.9.2"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.2",
        "@types/node": "^16.11.13",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    },
    "scripts": {
        "start": "tsc & node --experimental-json-modules build/main.js",
        "dev": "tsc & node --experimental-json-modules build/testMain.js",
        "prettier": "prettier --config .prettierrc src/**/*.ts --write",
        "build": "tsc",
        "deploy": "tsc & node --experimental-json-modules build/deployCommands.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sahneeintopf/DiscordTs-Bot.git"
    },
